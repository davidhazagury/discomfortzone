
<div class="conatiner">
  <%= form_tag events_path, method: :get do %>
  <div class="search-form-control form-group">
    <%= text_field_tag :query,
      params[:query],
      class: "form-control string required",
      placeholder: "Find an adventure"
    %>
    <%= submit_tag "Search", class: "btn btn-light-blue" %>
    </div>
  <% end %>
</div>
